<script>
		$(window).scroll(function(){
		//获取窗口已滚动的高度
		var windowScrollTop=$(window).scrollTop();
		var oTools=$(".back_top");
		//如果大约100PX，就渐显出“回到顶部”，否则即隐藏
		if(windowScrollTop>200)
		{
			oTools.fadeIn();
		}else{
			oTools.fadeOut();
		}
	});

	$(".back_top").click(function(){
		//点击“回到顶部”，滚动到顶部，并带动画效果
		$("html,body").animate({scrollTop:0},1000);
	});
