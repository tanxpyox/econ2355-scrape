	$(".flink a").each(function(index, element) {
		var curHref_ = $(this).attr("href");
		$(this).click(function(){
			if(curHref_.indexOf("http")>-1 && curHref_.indexOf("fmprc.gov.cn")<0 && curHref_.indexOf("mfa.gov.cn")<0)
