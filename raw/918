function search_check(){
	var sw = $("#sw").val();
	if(sw == "")
	{
		alert("请输入关键词！");
		return false;
	}
	else
	{
		window.location.href="/irs-c-web/search.shtml?code=17e50b77dab&searchWord="+sw;
		return true;
	}
}
