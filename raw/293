{
	var pmWidth = $("#News_Body_Txt_A").width();
	var pmHeight = pmWidth/1.744;
	$("#News_Body_Txt_A").find(".edui-upload-video").css("width",pmWidth);
	$("#News_Body_Txt_A").find(".edui-upload-video").css("height",pmHeight);
}
