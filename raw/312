$(".lan_btn img").each(function(index, element) {
    var lanDesc = $(this).attr("data-desc");
	if(lanDesc=="英文")
		$(this).attr("src","../../../images/en_btn_2023_1.png");
	if(lanDesc=="法文")
		$(this).attr("src","../../../images/fyr2023_btn_fr.png");
	if(lanDesc=="西文")
