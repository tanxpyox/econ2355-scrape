    var zylj_href = $(this).attr("href");
	zylj_href = zylj_href.replace(/<br>/g,"");
zylj_href = zylj_href.replace(/<br\/>/g,"");
	zylj_href = zylj_href.replace("　　","");
	$(this).attr("href",zylj_href);
});

